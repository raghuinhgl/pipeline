pipeline {
  agent none
  stages {
    stage ('build') {
    agent any
       steps {
          sh '''
                  echo "this is build stage"
                  sleep 2
             '''
             }
    }
    stage ('deploy') {
    agent { label 'javacod-s1' }
       steps {
          sh '''
                  echo "this is deploy stage"
                  sleep 2
             '''
             }
    }
    stage ('test') {
    agent any
       steps {
          sh '''
                  echo "this is test stage"
                  sleep 2
             '''
             }
    }
  }
}
